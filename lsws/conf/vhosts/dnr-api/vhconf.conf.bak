docRoot                   $VH_ROOT/dnr-api/public
enableGzip                1

errorlog $SERVER_ROOT/logs/$VH_NAME/error.log {
  useServer               0
  logLevel                ERROR
  rollingSize             1M
}

accesslog $SERVER_ROOT/logs/$VH_NAME/access.log {
  useServer               0
  rollingSize             10M
  keepDays                30
  compressArchive         1
}

scripthandler  {
  add                     lsapi:lsphp81 php
}

rewrite  {
  enable                  1
  autoLoadHtaccess        1
}

